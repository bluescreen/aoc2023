{
    "compilerOptions": {
      "lib": ["es2021"],
      "noEmit": true,
      "esModuleInterop": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      "isolatedModules": true,
      "incremental": true,
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "./*"
        ]
      }
    },
    "exclude": [
      "node_modules"
    ],
    "include": [
        "contentful/**/*.ts",
        "**/*.ts",
        "**/*.tsx"
    ],
    
  }
  